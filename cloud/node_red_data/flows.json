    {
        "id": "central_alarm_logic",
        "type": "function",
        "z": "6387abeabe798fe7",
        "name": "Lógica Central de Alarmas",
        "func": "const temp_alarm = flow.get('temp_alarm') || false;\nconst vapor_alarm = flow.get('vapor_alarm') || false;\nconst luz_alarm = flow.get('luz_alarm') || false;\nconst temp_prevista_alta = flow.get('hay_Riesgo') || false;\n\nlet buzzer_msg = null;\nlet semaforo_msg = null;\n\nif (temp_alarm || vapor_alarm || luz_alarm || temp_prevista_alta) {\n// Lógica del Buzzer\n    buzzer_msg = { payload: 'ALARM_ON' };\n} else {\n    buzzer_msg = { payload: 'ALARM_OFF' };\n}\n\n// Lógica del Semáforo con combinaciones\nlet semaforo_command = 0;\nif (temp_alarm) {\n    semaforo_command |= 4; // Red\n}\nif (vapor_alarm) {\n    semaforo_command |= 1; // Green\n}\nif (luz_alarm) {\n    semaforo_command |= 2; // Yellow\n}\nif (temp_prevista_alta) {\n    semaforo_command = 7; // All ON\n}\n\nsemaforo_msg = { payload: semaforo_command.toString() };\n\n// Se envían dos mensajes, uno para cada actuador\nreturn [buzzer_msg, semaforo_msg];",
        "outputs": 2,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 1200,
        "y": 280,
        "wires": [
            [
                "rbe_buzzer"
            ],
            [
                "rbe_semaforo"
            ]
        ]
    },
